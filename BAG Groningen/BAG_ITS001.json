                        "input": true,
                        "widget": "choicesjs",
                        "tableView": true,
                        "data": {
                            "values": [
                                {
                                    "label": "Verleende vergunning",
                                    "value": "verleendeVergunning"
                                },
                                {
                                    "label": "Terugmelding",
                                    "value": "terugmelding"
                                }
                            ]
                        },
                        "validate": {
                            "required": true
                        },
                        "conditional": {
                            "json": {
                                "and": [
                                    {
                                        "var": "data.<RB_AdviseurAkkoord1>" //boolean
                                    },
                                    {
                                        "or": [
                                            {
                                                "!": {
