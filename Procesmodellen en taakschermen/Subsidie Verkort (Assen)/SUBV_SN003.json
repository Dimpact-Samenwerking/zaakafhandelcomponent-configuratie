{
  "name": "SUBV_SN003",
  "title": "Keuze specialisten beoordeling ontvankelijkheid",
  "display": "form",
  "type": "form",
  "components": [
    {
      "html": "<p>Beoordeling van de ontvankelijkheid op basis van de indiening vereisten voor de aangevraagde activiteiten.</p>",
      "label": "Content",
      "refreshOnChange": false,
      "key": "CO_infoTekst",
      "type": "content",
      "input": false,
      "tableView": false
    },
    {
      "label": "RegelingenVordeningen",
      "optionsLabelPosition": "right",
      "inline": true,
      "hideLabel": true,
      "tableView": false,
      "values": [
        {
          "label": "Regeling",
          "value": "regeling",
          "shortcut": ""
        },
        {
          "label": "Verordening",
          "value": "verordening",
          "shortcut": ""
        }
      ],
      "validateWhenHidden": false,
      "key": "regelingenVordeningen",
      "type": "radio",
      "input": true
    },
    {
      "label": "Regelingen",
      "widget": "html5",
      "tableView": true,
      "data": {
        "values": [
          {
            "label": "Regeling A",
            "value": "regelingA"
          },
          {
            "label": "Regeling B",
            "value": "regelingB"
          },
          {
            "label": "Regeling C",
            "value": "regelingC"
          },
          {
            "label": "Regeling D",
            "value": "regelingD"
          }
        ]
      },
      "validateWhenHidden": false,
      "key": "SB_regelingen",
      "conditional": {
        "show": true,
        "when": "regelingenVordeningen",
        "eq": "regeling"
      },
      "type": "select",
      "input": true
    },
    {
      "label": "Verordening",
      "widget": "html5",
      "tableView": true,
      "data": {
        "values": [
          {
            "label": "Verordening 1",
            "value": "verordening1"
          },
          {
            "label": "Verordening 2",
            "value": "verordening2"
          },
          {
            "label": "Verordening3",
            "value": "verordening3"
          }
        ]
      },
      "validateWhenHidden": false,
      "key": "SB_verordening",
      "conditional": {
        "show": true,
        "when": "regelingenVordeningen",
        "eq": "verordening"
      },
      "type": "select",
      "input": true
    },
    {
      "label": "Opvragen ontbrekende stukken",
      "optionsLabelPosition": "right",
      "inline": true,
      "tableView": false,
      "values": [
        {
          "label": "Ja",
          "value": "ja",
          "shortcut": ""
        },
        {
          "label": "Nee",
          "value": "nee",
          "shortcut": ""
        }
      ],
      "validateWhenHidden": false,
      "key": "RB_opvragenOntbrekendeStukken",
      "type": "radio",
      "input": true
    },
    {
      "label": "Geef hier de ontbrekende stukken aan",
      "applyMaskOn": "change",
      "autoExpand": false,
      "tableView": true,
      "validateWhenHidden": false,
      "key": "TA_geefHierDeOntbrekendeStukkenAan",
      "conditional": {
        "show": true,
        "when": "RB_opvragenOntbrekendeStukken",
        "eq": "ja"
      },
      "type": "textarea",
      "rows": 5,
      "input": true
    },
    {
      "label": "Buttons",
      "key": "buttons",
      "type": "container",
      "input": false,
      "tableView": false,
      "components": [
        {
          "type": "columns",
          "columns": [
            {
              "width": "auto",
              "components": [
                {
                  "key": "save",
                  "label": "Opslaan",
                  "type": "button",
                  "input": true,
                  "action": "saveState",
                  "state": "draft",
                  "tableView": false
                }
              ],
              "offset": 0,
              "push": 0,
              "pull": 0,
              "size": "md",
              "currentWidth": "auto"
            },
            {
              "width": "auto",
              "components": [
                {
                  "key": "submit",
                  "label": "Opslaan & afronden",
                  "type": "button",
                  "input": true,
                  "disableOnInvalid": true,
                  "tableView": false
                }
              ],
              "offset": 0,
              "push": 0,
              "pull": 0,
              "size": "md",
              "currentWidth": "auto"
            }
          ],
          "input": false,
          "key": "columns",
          "tableView": false,
          "label": "Columns"
        }
      ]
    }
  ]
}